#include "block.h"

/*
 * ROM spec file
 */
beginseg
	name "code"
	flags BOOT OBJECT
	entry boot
	address 0x80200000
	stack bootStack + STACKSIZE
	include "codesegment.o"
	include "$(ROOT)/usr/lib/PR/rspboot.o"
	include "$(ROOT)/usr/lib/PR/gspFast3D.o"
	include "$(ROOT)/usr/lib/PR/gspFast3D.dram.o"
	include "$(ROOT)/usr/lib/PR/gspTurbo3D.o"
endseg

beginseg
	name "zbuffer"
	flags OBJECT
	address 0x803da800
	include "zbuffer.o"
endseg

beginseg
	name "cfb"
	flags OBJECT
	address 0x8014f800
	include "cfb.o"
endseg

beginwave
	name "blocks"
	include "code"
	include "cfb"
	include "zbuffer"
endwave
