#include "def.h"
/*
 * ROM spec file
80000450  code
80150a20  gfx
802da800  zbuffer
8036a000  cfb

 */
beginseg
	name "code"
	flags BOOT OBJECT
	entry boot
	stack bootStack + STACKSIZE
	include "codesegment.o"
	include "$(ROOT)/usr/lib/PR/rspboot.o"
	include "$(ROOT)/usr/lib/PR/aspMain.o"
	include "$(ROOT)/usr/lib/PR/gspF3DEX.fifo.o"
	include "$(ROOT)/usr/lib/PR/gspF3DLX.fifo.o"
	include "$(ROOT)/usr/lib/PR/gspF3DLX.Rej.fifo.o"
	include "$(ROOT)/usr/lib/PR/gspF3DLP.Rej.fifo.o"
	include "$(ROOT)/usr/lib/PR/gspL3DEX.fifo.o"
	include "$(ROOT)/usr/lib/PR/gspLine3D.fifo.o"
endseg

beginseg
	name "cfb"
	flags OBJECT
	address 0x8036a000
	include "cfb.o"
endseg

beginseg
	name "zbuf"
	flags OBJECT
	address	0x802da800
	include	"zbuf.o"
endseg

beginseg
	name "gfxdlist"
	after code
	flags OBJECT
	include "gfxdlistbuf.o"

endseg

beginseg
	name "fiter1"
	number STATIC_SEGMENT 
	flags OBJECT
	include "fiter_1s1.o"
endseg

beginseg
	name "fiter2"
	number STATIC_SEGMENT 
	flags OBJECT
	include "fiter_2s1.o"
endseg
beginseg
	name "cont"
	number STATIC_SEGMENT 
	flags OBJECT
	include "cont_finish.o"
endseg

beginseg
	name "midibank"
	flags RAW
	include "$(ROOT)/usr/lib/PR/soundbanks/GenMidiBank.ctl"
endseg

beginseg
	name "miditable"
	flags RAW
	include "$(ROOT)/usr/lib/PR/soundbanks/GenMidiBank.tbl"
endseg

beginseg
	name "seq"
	flags RAW
	include "$(ROOT)/usr/lib/PR/sequences/ship.sbk"
/*	include "$(ROOT)/usr/lib/PR/sequences/cmpship.sbk"*/

endseg

beginseg
	name "sfxbank"
	flags RAW
	include "$(ROOT)/usr/lib/PR/soundbanks/sfx.ctl"
endseg

beginseg
	name "sfxtable"
	flags RAW
	include "$(ROOT)/usr/lib/PR/soundbanks/sfx.tbl"
endseg

beginwave
	name "nnsample1"
	include "code"
	include "gfxdlist"
/*	include "graphic" */
	include "miditable"
	include "midibank"
	include "sfxbank"
	include "sfxtable"
	include "seq"
	include	"zbuf"
	include "cfb"
	include "fiter1"
	include "fiter2"
	include "cont"
endwave




