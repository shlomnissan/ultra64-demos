#include "fill.h"

/*
 * ROM spec file
 */
beginseg
	name "code"
	flags BOOT OBJECT
	entry boot
	address 0x80200000
	stack bootStack + STACKSIZE
	include "codesegment.o"
	include "$(ROOT)/usr/lib/PR/rspboot.o"
	include "$(ROOT)/usr/lib/PR/gspFast3D.o"
	include "$(ROOT)/usr/lib/PR/gspFast3D.dram.o"
endseg

beginseg
	name "zbuffer"
	flags OBJECT
	address 0x80000800
	include "zbuffer.o"
endseg

beginseg
	name "cfb"
	flags OBJECT
	address 0x8018f800
	include "cfb.o"
endseg

beginseg
    name "rsp_cfb"
    flags OBJECT
    number CFB_SEGMENT
    include "rsp_cfb.o"
endseg

beginwave
	name "fill"
	include "code"
	include "cfb"
	include "rsp_cfb"
	include "zbuffer"
endwave
